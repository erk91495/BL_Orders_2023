<Page
    x:Class="Bl_Orders_2023.Views.FillOrdersPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <RelativePanel x:Name="ContentArea">
        <ComboBox x:Uid="Order_Search"/>
        <TextBox x:Uid="Order_ID" RelativePanel.RightOf="Order_Search"/>
        <TextBox x:Uid="Cust_ID" RelativePanel.RightOf="Order_ID"/>
        <TextBox x:Uid="Customer_Name" RelativePanel.RightOf="Cust_ID"/>
        <TextBox x:Uid="Customer_Class" RelativePanel.RightOf="Customer_Name"/>
        <ItemsRepeater></ItemsRepeater>
    </RelativePanel>
</Page>
